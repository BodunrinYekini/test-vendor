<!-- Subsystem log settings -->
<subsys_settings>

  <global>
    <media>data</media>
  </global>

  <cellular_modem>
    <log_mode>normal</log_mode>
    <dest>local</dest>
    <compression>none</compression>
    <ps>on</ps>
    <ps_level>3</ps_level>
    <trace_module>FFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFF</trace_module>
    <sap_list>FFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFF</sap_list>
    <ps_pcm>off</ps_pcm>
    <ip>on</ip>
    <ip_len>60</ip_len>
    <dump>on</dump>
    <minidump>on</minidump>
    <etb>on</etb>
    <phy_dest>ps</phy_dest>
    <phy>on</phy>
    <phy_level>1</phy_level>
    <phy_pcm>off</phy_pcm>
    <iq>off</iq>
    <iq_item>0</iq_item>
    <iq_pos>0</iq_pos>
    <io>
      <avg_ratio>48</avg_ratio>
      <log_rate>15</log_rate>
      <buf_size>256</buf_size>
      <buf_num>24</buf_num>
      <log_commit_threshold>128</log_commit_threshold>
      <buf_commit_threshold>224</buf_commit_threshold>
    </io>
    <storage>
      <ext_cap>10240</ext_cap>
      <int_cap>10240</int_cap>
      <file>256</file>
      <recycle>on</recycle>
    </storage>
    <fnodel>off</fnodel>
  </cellular_modem>

  <connectivity>
    <dest>local</dest>
    <compression>none</compression>
    <wifi_bt>on</wifi_bt>
    <wifi_bt_level>3</wifi_bt_level>
    <gnss>on</gnss>
    <wcn_reset_state>dump</wcn_reset_state>
    <storage>
      <ext_cap>0</ext_cap>
      <int_cap>512</int_cap>
      <file>128</file>
      <recycle>on</recycle>
    </storage>
  </connectivity>

  <audio_dsp>
    <log>on</log>
    <pcm>on</pcm>
    <dest>local</dest>
    <agdspdest>ap</agdspdest>
    <storage>
      <ext_cap>0</ext_cap>
      <int_cap>256</int_cap>
      <file>128</file>
      <recycle>on</recycle>
    </storage>
  </audio_dsp>

  <pmsh>
    <log>on</log>
    <compression>none</compression>
    <storage>
      <ext_cap>0</ext_cap>
      <int_cap>256</int_cap>
      <file>128</file>
      <recycle>on</recycle>
    </storage>
  </pmsh>

  <sensor_hub>
    <log>on</log>
    <compression>none</compression>
    <storage>
      <ext_cap>0</ext_cap>
      <int_cap>256</int_cap>
      <file>128</file>
      <recycle>on</recycle>
    </storage>
  </sensor_hub>

</subsys_settings>
